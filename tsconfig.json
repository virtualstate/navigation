{
    "compilerOptions": {
        "module": "esnext",
        "lib": [
            "es2018",
            "esnext",
            "dom",
            "dom.iterable"
        ],
        "types": [
          "node",
          "urlpattern-polyfill"
        ],
        "esModuleInterop": true,
        "target": "esnext",
        "noImplicitAny": true,
        "downlevelIteration": true,
        "moduleResolution": "node",
        "declaration": true,
        "sourceMap": true,
        "outDir": "esnext",
        "baseUrl": "./src",
        "jsx": "react",
        "jsxFactory": "h",
        "jsxFragmentFactory": "createFragment",
        "allowJs": true,
        "paths": {
        }
    },
    "include": [
        "src/routes/*",
        "src/**/*"
    ],
    "typeRoots": [
        "src/types"
    ],
    "exclude": [

    ]
}
